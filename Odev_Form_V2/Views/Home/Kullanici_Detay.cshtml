@model Odev_Form_V2.Models.ViewModel.Home_Index_View_Model
@{
    ViewBag.Title = "Kullanici_Detay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="fb-root"></div>
 
<div class="page-container clearfix" id="main-wrap">

    <div id="left-frame">

     
        <div class="titles">




            @if (Session["KID"] != null)
            {
                <p> @{ int id = Convert.ToInt32(Session["KID"].ToString()); }</p>
                foreach (var item in Model.TBL_Kullanici)
                {

                    if (item.ID == id)
                    {
                        <h5 style="color:red;">
                            <center>
                                <br />
                                Şifre Değiştir
                            </center>
                        </h5>
                        <hr />
                        <form action="/Home/Parola_Degistir" method="post">
                            <div class="col-md-12 row form-group">

                                <input type="hidden" name="ID" value="@item.ID" />

                                <div class="col-md-6 form-group">
                                    <label>Şifre Değiştir</label>
                                </div>
                                <div class="col-md-6 form-group">
                                    <input type="text" name="pass" class="form-control" required />
                                </div>
                                <div class="col-md-12 form-group">
                                    <button type="submit" class=" btn btn-info fa fa-save col-md-12">Kaydet</button>
                                </div>
                            </div>
                        </form>
                    }
                }
            }

            <script>
                if (window.sessionStorage) {
                    var spEx = sessionStorage.getItem("sp");
                    var spNew = 'gundem';
                    if (spEx == spNew) {
                        document.getElementById("sol-index-list").scrollTop = sessionStorage.getItem("sideScrollPosition");
                    } else {
                        sessionStorage.setItem("sp", spNew);
                        sessionStorage.setItem("sideScrollPosition", 0);
                    }
                    spEx = spNew = null;
                }
            </script>
        </div>
    </div><!--left-frame sonu-->

    <div id="content">


        <!-- reklam alani -->
        <!-- Admatic masthead 970x250 Ad Code START -->
        <ins data-publisher="adm-pub-166715399597" data-ad-type="masthead" class="adm-ads-area" data-ad-network="163354846560" data-ad-sid="501" data-ad-width="970" data-ad-height="250"></ins>
        <script src="../../../cdn2.admatic.com.tr/showad/showad.js" async></script>
        <!-- Admatic masthead 970x250 Ad Code END -->

        <div id="right-container" class="clearfix ">



            <div id="middle-block" class="col-md-9">

                @foreach (var item in Model.TBL_Kullanici)
                {
                    <div class="wbox clearfix asfasdfsdfsa">
                        <h1 class="title" id="baslik_24546969">
                            <a href="/Home/Entri_Detay/@item.ID">@item.Ad_Soyad Entrileri</a>
                            <div class="title_sag">
                            </div>
                        </h1>
                    </div>
                }

                @foreach (var item in Model.TBL_Entri)
                {


                    <ol class="entry-list" start="2">
                        <li class="entry wbox entry_210099612" value="1">
                            <span class="list_value">1.</span>
                            <div class="entry-text clearfix">
                                <div class="entry-puan">
                                    <img height="40" src="data:image/png;base64," alt="Alternate Text" />
                                </div>


                                <div class="entry-text-wrap " data-leng="27">
                                    @item.Aciklama
                                </div>


                                <div class="entry-options yeni ">
                                    <div style="display: inline-block;float: right;">
                                        @foreach (var x in Model.TBL_Kullanici)
                                        {

                                            <a class="username" style="display:inline-block; clear:both; float:none;" href="/Home/Kullanici_Detay/@x.ID">@x.KAD</a>


                                        }
                                        <br />
                                        <a href="index.html" style="display:inline-block; float:none; clear:both;" class="entry-tarih"><small>@item.Eklenme_Tarihi.ToString("dd/MM/yyyy")</small></a>
                                        <div class="clearfix"></div>
                                    </div>

                                </div><!-- entry-options -->

                            </div><!-- entry-text clearfix -->
                        </li><!-- entry wbox entry_210099612 -->


                    </ol><!-- entry-list -->
                }




            </div>

            <div id="middle-block" class="col-md-9">

                @foreach (var item in Model.TBL_Kullanici)
                {
                    <div class="wbox clearfix asfasdfsdfsa">
                        <h1 class="title" id="baslik_24546969">
                            <a href="/Home/Entri_Detay/@item.ID">@item.Ad_Soyad Konuları</a>
                            <div class="title_sag">
                            </div>
                        </h1>
                    </div>
                }

                @foreach (var item in Model.TBL_Konu)
